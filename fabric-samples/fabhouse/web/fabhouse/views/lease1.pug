extends ../node_modules/jade-bootstrap/_bootstrap
append styles
        link(href='/stylesheets/signup.css', rel='stylesheet')
       

        script(src='/javascripts/jquery.js', type='text/javascript')
        script(src='/javascripts/rent.js', type='text/javascript')

block body
        .site-wrapper
                .site-wrapper-inner
                        .cover-container
                                .masthead.clearfix
                                        .inner
                                                h3.masthead-brand FabHouse
                                                nav
                                                        ul.nav.masthead-nav
                                                                li
                                                                        a(href="/") 主页
                                                                li.active
                                                                        a(href="/lease1?name="+name+",idcard="+idcard) 租房合约
                                                                li
                                                                        a(href="#") 关于
 
        .container
                div.form-signin
                                button.btn.btn-lg.btn-primary.btn-block(onclick="toownerrent()") 查看租房合约